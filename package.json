{
  "name": "picross",
  "version": "0.0.3",
  "description": "Picross 3D in the browser!",
  "main": "picross.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot",
    "build": "webpack --mode production",
    "debug": "webpack --mode development",
    "watch": "webpack -w",
    "test": "jest --watchAll"
  },
  "keywords": [
    "Picross",
    "3D",
    "nonograms",
    "hanjie",
    "griddler",
    "paint-by-number",
    "puzzle"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/redux": "^3.6.0",
    "@types/three": "^0.93.27",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "file-saver": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "style-loader": "^0.23.1",
    "three": "^0.102.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "~3.3.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "author": "Nathan Soufflet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathsou/Picross3D"
  },
  "homepage": "https://nathsou.github.io/Picross3D/"
}