{
  "name": "picross",
  "version": "0.0.1",
  "description": "Picross 3D in the browser!",
  "main": "picross.js",
  "scripts": {
    "start": "webpack-dev-server --mode development -d --host 0.0.0.0",
    "build": "webpack --mode production",
    "debug": "webpack --mode development",
    "watch": "webpack -w",
    "test": "jest --watchAll"
  },
  "keywords": [
    "Picross",
    "3D",
    "nonograms",
    "hanjie",
    "griddler",
    "paint-by-number",
    "puzzle"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/three": "^0.93.27",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "three": "^0.102.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.2.1",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "@types/file-saver": "^2.0.0",
    "file-saver": "^2.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "author": "Nathan Soufflet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathsou/Picross3D"
  }
}